{% extends 'base.html.twig' %}

{% block body %}

    <ul>
        <li>
            <a href="{{ path('storeddatabase_new') }}">Create a new storedDatabase</a>
        </li>
    </ul>

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Relativepath</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for storedDatabase in storedDatabases %}
            <tr>
                <td><a href="{{ url('storeddatabase_load', { 'file' : storedDatabase }) }}">{{ storedDatabase }}</a></td>
                <td>
                    <ul>
                        <li><a href="{{ url('storeddatabase_edit', { 'file' : storedDatabase }) }}">update</a></li>
                        <li><a href="{{ url('storeddatabase_delete', { 'file' : storedDatabase }) }}">delete</a></li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
