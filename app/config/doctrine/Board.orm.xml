<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppBundle\Repository\BoardRepository" name="AppBundle\Entity\Board" table="board">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="owner" type="string" column="owner" length="255" precision="0" scale="0" nullable="false"/>
    <field name="wipLimit" type="integer" column="wip_limit" precision="0" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
