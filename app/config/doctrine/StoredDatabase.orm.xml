<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppBundle\Repository\StoredDatabaseRepository" name="AppBundle\Entity\StoredDatabase" table="stored_database">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="relativePath" type="string" column="relative_path" length="255" precision="0" scale="0" nullable="false"/>
    <field name="originalName" type="string" column="original_name" length="255" precision="0" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>
